{
	"name": "react-project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"browserslist": [
		"last 2 version",
		"ie >= 11",
		"iOS >= 8.1",
		"Android >= 4.4"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "prettier \"**/!(*.md)\" --write --ignore-unknown --ignore-path .gitignore",
		"lint:html": "markuplint \"**/*.html\" & markuplint \"**/*.html\" --fix",
		"lint:css": "stylelint \"**/*.{css,scss}\" --fix",
		"lint:javascript": "eslint \"**/*.js\" --fix",
		"lint:all": "npm run lint:html & npm run lint:css & npm run lint:javascript",
		"lint&format": "npm run lint:all & npm run format"
	},
	"dependencies": {
		"@gsap/react": "^2.1.0",
		"@studio-freight/lenis": "^1.0.29",
		"gsap": "^3.12.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rellax": "^1.12.1"
	},
	"devDependencies": {
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"markuplint": "^3.15.0",
		"postcss": "^8.4.32",
		"postcss-sort-media-queries": "^5.2.0",
		"sass": "^1.69.5",
		"stylelint": "^15.11.0",
		"stylelint-config-property-sort-order-smacss": "^9.1.0",
		"stylelint-config-recommended-scss": "^13.1.0",
		"stylelint-order": "^6.0.3",
		"stylelint-scss": "^5.3.1",
		"vite": "^5.0.8"
	}
}
